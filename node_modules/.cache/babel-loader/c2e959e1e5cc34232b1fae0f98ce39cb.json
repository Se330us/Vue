iv>\r\n      <div><strong>Описание:</strong> {{ post.body }}</div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      posts: [\r\n        { id: 1, title: \"Post\", body: \"Text\" },\r\n        { id: 2, title: \"Post 2\", body: \"Text 2\" },\r\n        { id: 3, title: \"Post 3\", body: \"Text 3\" },\r\n      ],\r\n      title: \"\",\r\n      body: \"\",\r\n    };\r\n  },\r\n  methods: {\r\n    createPost() {\r\n        const newPost = {\r\n            id: Date.now(),\r\n            title: this.title,\r\n            body: this.body\r\n        }\r\n        this.posts.push(newPost);\r\n        this.title = '';\r\n        this.body = ''\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style>\r\n* {\r\n  margin: 0;\r\n  padding: 0;\r\n  box-sizing: border-box;\r\n}\r\n.app {\r\n  padding: 10px;\r\n}\r\n.post {\r\n  padding: 15px;\r\n  border: 2px solid black;\r\n  margin-top: 10px;\r\n}\r\nform {\r\n  display: flex;\r\n  flex-direction: column;\r\n}\r\n.input {\r\n  width: 100%;\r\n  border: 1px solid black;\r\n  padding: 10px;\r\n  margin-top: 10px;\r\n}\r\n.btn {\r\n  margin-top: 15px;\r\n  align-self: flex-end;\r\n  padding: 10px 15px;\r\n  background: none;\r\n  color: black;\r\n  border: 1px solid black;\r\n}\r\n</style>"],"sourceRoot":""}]}